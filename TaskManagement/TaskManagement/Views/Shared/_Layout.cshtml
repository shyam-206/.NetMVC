    <!-- Page level plugins -->
    <script src="~/Content/Theme/Dashboard/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Theme/Dashboard/js/demo/chart-area-demo.js"></script>
    <script src="~/Content/Theme/Dashboard/js/demo/chart-pie-demo.js"></script>
    
    <script src="https://cdn.datatables.net/2.0.5/js/dataTables.min.js"></script>

</body>

